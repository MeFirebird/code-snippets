
   /**
     * 获取对象的类属性（表示对象的类型信息）
     * @param o
     * @returns {*}
     */
    function classof(o){
        if(o === null){
            return 'Null'
        }
        if(o === undefined){
            return "Undefined";
        }
        return Object.prototype.toString.call(o).slice(8,-1);
    }
